{
  "compilerOptions": {
    "target": "ES2016",
    "module": "AMD",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outFile": "./dst/js/dummy.js",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "./ts/*.ts",
    "./ts-viewer/*.ts",
  ]
}